{
	"comments": {
		// symbol used for single line comment. Remove this entry if your language does not support line comments
		"lineComment": "!"
	},
	// symbols used as brackets
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		{ "open": "'", "close": "'", "notIn": ["string", "comment"] }
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["'", "'"]
	],
	"folding": {
		"markers": {
			"start": "^\\s*s*!region\\b",
			"end": "^\\s*!endregion\\b"
		}
	}
}